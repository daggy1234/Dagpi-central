{
  "name": "dagpi-central",
  "version": "2.2.1",
  "description": "Central management Api",
  "main": "app/server.ts",
  "repository": "https://github.com/Daggy1234/Dagpi-central.git",
  "author": "Daggy1234 <arnav.jindal7@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier:fix": "prettier --parser typescript --write \"app/**/*.ts\"",
    "prod": "prisma generate && cross-env NODE_ENV=production node dist/server.js",
    "start": "cross-env NODE_ENV=local nodemon -V --exec ts-node app/server.ts",
    "dev": "cross-env NODE_ENV=development nodemon -V --exec ts-node --project tsconfig.json app/server.ts"
  },
  "private": false,
  "dependencies": {
    "@prisma/client": "^2.23.0",
    "@sentry/node": "^6.3.6",
    "@sentry/tracing": "^6.3.6",
    "aws-sdk": "^2.889.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "csprng": "^0.1.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "prisma": "^2.23.0",
    "stripe": "^8.149.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.41",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unicorn": "^30.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
